<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AboutActivity">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btn_sms"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="32dp"
        android:clickable="true"
        android:contentDescription="@string/sms"
        android:src="@drawable/ic_sms"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.983"
         />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btn_Brauser"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:contentDescription="@string/brauser"
        android:src="@drawable/ic_brouser"
        app:layout_constraintBottom_toBottomOf="@+id/btn_sms"
        app:layout_constraintStart_toEndOf="@+id/btn_sms" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btn_map"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:contentDescription="@string/map"
        android:src="@drawable/ic_map"
        app:layout_constraintBottom_toBottomOf="@+id/btn_Brauser"
        app:layout_constraintStart_toEndOf="@+id/btn_Brauser" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btn_call"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:contentDescription="@string/call"
        android:src="@drawable/ic_call"
        app:layout_constraintBottom_toBottomOf="@+id/btn_map"
        app:layout_constraintStart_toEndOf="@+id/btn_map" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btn_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:contentDescription="@string/email"
        android:src="@drawable/ic_email"
        app:layout_constraintBottom_toBottomOf="@+id/btn_call"
        app:layout_constraintStart_toEndOf="@+id/btn_call" />
</androidx.constraintlayout.widget.ConstraintLayout>